{
    "version": 2,
    "builds": [
      {
        "src": "client/public/index.html",
        "use": "@vercel/static"
      },
      {
        "src": "api/**/*.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/$1"
      }
    ]
  }
  